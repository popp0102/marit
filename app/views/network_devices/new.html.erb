<form action="/network_devices">
  <div class="form-group">
    <label for="mac">MAC Address:</label>
    <input type="text" class="form-control" placeholder="Enter MAC Address" id="network_device_mac_address">
  </div>
  <div class="form-group">
    <label for="name">Name:</label>
    <input type="text" class="form-control" placeholder="Enter Name" id="network_device_name">
  </div>
  <button type="submit" class="btn btn-primary">Add</button>
</form>

